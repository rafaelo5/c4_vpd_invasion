vpd_data_means <- summarise(vpd_data_groupby,
VPDleaf_mean = mean(VPDleaf, na.rm = T),
Tleaf_mean = mean(Tleaf, na.rm = T),
A_mean = mean(A, na.rm = T),
gsw_mean = mean(gsw, na.rm = T),
Ci_mean = mean(Ci, na.rm = T),
E_mean = mean(E, na.rm = T))
